<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.SenimoApplication"
        android:usesCleartextTraffic="true"
        tools:targetApi="34">
        <activity
            android:name=".Login.Activity_login.SplashActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Login.Activity_login.LoginActivity"
            android:exported="false" />
        <activity
            android:name=".Login.Activity_login.EnterMyInfoActivity"
            android:windowSoftInputMode="adjustPan"
            android:exported="false" />
        <activity
            android:name=".MainPage.Activity_main.MainActivity"
            android:exported="false" />
        <activity
            android:name=".MainPage.Activity_main.SearchActivity"
            android:exported="false" />
        <activity android:name=".MainPage.Activity_main.CreateMeetingActivity"
            android:exported="false"/>
        <activity
            android:name=".Club.Activity_club.ScheduleActivity"
            android:exported="false" />
        <activity
            android:name=".Club.Activity_club.ClubActivity"
            android:exported="false"/>


    </application>

</manifest>