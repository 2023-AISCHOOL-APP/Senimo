<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <androidx.cardview.widget.CardView
        android:id="@+id/cardView"
        android:layout_width="0dp"
        android:layout_height="150dp"
        android:layout_marginStart="10dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="10dp"
        android:layout_marginBottom="8dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.516"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <TextView
                android:id="@+id/tv_C_ScheduleName"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="10dp"
                android:layout_marginTop="36dp"
                android:layout_marginEnd="17dp"
                android:layout_marginBottom="86dp"
                android:fontFamily="@font/pretendard_semibold"
                android:text="동명동 맛집 탐방🍗"
                android:textColor="@color/txt_black"
                android:textSize="19sp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toEndOf="@+id/Img_M_Meeting"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/tvRealFee"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="6dp"
                android:fontFamily="@font/pretendard_medium"
                android:text="20000원"
                android:textColor="@color/txt_gray90"
                android:textSize="13sp"
                app:layout_constraintBottom_toBottomOf="@+id/imageView4"
                app:layout_constraintStart_toEndOf="@+id/imageView4"
                app:layout_constraintTop_toTopOf="@+id/imageView4" />

            <TextView
                android:id="@+id/tvRealTime"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="6dp"
                android:fontFamily="@font/pretendard_medium"
                android:text="2023.10.30(월) 19:00"
                android:textColor="@color/txt_gray90"
                android:textSize="13sp"
                app:layout_constraintBottom_toBottomOf="@+id/imageView"
                app:layout_constraintStart_toEndOf="@+id/imageView"
                app:layout_constraintTop_toTopOf="@+id/imageView" />

            <TextView
                android:id="@+id/tvRealLoca"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="6dp"
                android:fontFamily="@font/pretendard_medium"
                android:text="광주 동구 동명동 야간비행"
                android:textColor="@color/txt_gray90"
                android:textSize="13sp"
                app:layout_constraintBottom_toBottomOf="@+id/imageView2"
                app:layout_constraintStart_toEndOf="@+id/imageView2" />

            <com.google.android.material.imageview.ShapeableImageView
                android:id="@+id/Img_M_Meeting"
                android:layout_width="wrap_content"
                android:layout_height="0dp"
                android:adjustViewBounds="true"
                android:backgroundTintMode="add"
                android:scaleType="centerCrop"
                android:src="@drawable/img_sample"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:shapeAppearanceOverlay="@style/RoundedImageView" />

            <TextView
                android:id="@+id/tv_C_dday"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@drawable/dday_normal"
                android:fontFamily="@font/pretendard_regular"
                android:paddingLeft="8dp"
                android:paddingTop="3dp"
                android:paddingRight="8dp"
                android:paddingBottom="3dp"
                android:text="D-20"
                android:textColor="@color/bg_darkgray10"
                android:textSize="10dp"
                app:layout_constraintBottom_toBottomOf="@+id/tv_C_State"
                app:layout_constraintStart_toStartOf="@+id/tv_C_ScheduleName"
                app:layout_constraintTop_toTopOf="@+id/tv_C_State" />

            <TextView
                android:id="@+id/tv_C_State"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="4dp"
                android:layout_marginTop="10dp"
                android:layout_marginBottom="10dp"
                android:background="@drawable/tag_open"
                android:fontFamily="@font/pretendard_regular"
                android:paddingLeft="8dp"
                android:paddingTop="3dp"
                android:paddingRight="8dp"
                android:paddingBottom="3dp"
                android:text="모집중"
                android:textColor="@color/tag_red"
                android:textSize="10sp"
                app:layout_constraintBottom_toTopOf="@+id/tv_C_ScheduleName"
                app:layout_constraintStart_toEndOf="@+id/tv_C_dday"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.0" />

            <TextView
                android:id="@+id/tv_C_allMember"
                android:layout_width="70dp"
                android:layout_height="wrap_content"
                android:alpha="0.8"
                android:background="@drawable/member_tag_shape"
                android:fontFamily="@font/pretendard_semibold"
                android:paddingStart="24dp"
                android:paddingTop="5dp"
                android:paddingRight="5dp"
                android:paddingBottom="5dp"
                android:text="17/20"
                android:textColor="@color/bg_darkgray70"
                app:layout_constraintEnd_toEndOf="@+id/Img_M_Meeting"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="@+id/Img_M_Meeting" />

            <ImageView
                android:id="@+id/imageView3"
                android:layout_width="14dp"
                android:layout_height="14dp"
                android:layout_marginStart="5dp"
                app:layout_constraintBottom_toBottomOf="@+id/tv_C_allMember"
                app:layout_constraintStart_toStartOf="@+id/tv_C_allMember"
                app:layout_constraintTop_toTopOf="@+id/tv_C_allMember"
                app:layout_constraintVertical_bias="0.5"
                app:srcCompat="@drawable/ic_member" />

            <ImageView
                android:id="@+id/imageView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="15dp"
                app:layout_constraintStart_toStartOf="@+id/tv_C_ScheduleName"
                app:layout_constraintTop_toBottomOf="@+id/tv_C_ScheduleName"
                app:srcCompat="@drawable/s_time" />

            <ImageView
                android:id="@+id/imageView2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="8dp"
                app:layout_constraintStart_toStartOf="@+id/imageView"
                app:layout_constraintTop_toBottomOf="@+id/imageView"
                app:srcCompat="@drawable/s_loca" />

            <ImageView
                android:id="@+id/imageView4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="8dp"
                android:layout_marginBottom="10dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toStartOf="@+id/imageView2"
                app:layout_constraintTop_toBottomOf="@+id/imageView2"
                app:srcCompat="@drawable/s_money" />
        </androidx.constraintlayout.widget.ConstraintLayout>

    </androidx.cardview.widget.CardView>
</androidx.constraintlayout.widget.ConstraintLayout>